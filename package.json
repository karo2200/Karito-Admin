{
	"name": "web-boilerplate",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "node -e \"if (process.env.NODE_ENV !== 'production') { require('child_process').execSync('husky install', { stdio: 'inherit' }); }\"",
		"dev": "node prebuild && next dev -p 4500",
		"qa": "node prebuild -qa && next dev -p 4600",
		"build": "npm run generate && node prebuild && next build",
		"build:prd": "npm run generate && node prebuild -prd && next build",
		"build:qa": "npm run generate && node prebuild -qa && next build",
		"build:stg": "npm run generate && node prebuild -stg && next build",
		"start": "serve -s out -l 4500",
		"generate": "graphql-codegen --config codegen.yml",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
		"lint": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
		"lint-staged": "lint-staged"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.879.0",
		"@aws-sdk/xhr-http-handler": "^3.873.0",
		"@azure/storage-blob": "^12.12.0",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@hookform/resolvers": "^2.9.10",
		"@mui/base": "5.0.0-beta.25",
		"@mui/icons-material": "^5.14.11",
		"@mui/lab": "^5.0.0-alpha.115",
		"@mui/material": "^5.11.4",
		"@tanstack/react-query": "^4.22.0",
		"@tanstack/react-query-devtools": "^4.22.0",
		"@types/nprogress": "^0.2.0",
		"axios": "^1.11.1",
		"chart.js": "^4.4.6",
		"dayjs": "^1.11.7",
		"echarts-for-react": "^3.0.2",
		"firebase": "^12.2.1",
		"generate-graphql": "^1.0.1",
		"graphql": "^16.6.0",
		"graphql-request": "^7.2.0",
		"iconsax-react": "^0.0.8",
		"jalaali-js": "^1.2.8",
		"jwt-decode": "^3.1.2",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"next": "^13.1.1",
		"nookies": "^2.5.2",
		"notistack": "^2.0.8",
		"nprogress": "^0.2.0",
		"ra-data-graphql": "^4.6.0",
		"react": "18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "18.2.0",
		"react-dropzone": "^14.3.8",
		"react-hook-form": "^7.41.5",
		"react-lazy-load-image-component": "^1.5.6",
		"react-leaflet": "^4.2.1",
		"react-leaflet-markercluster": "^4.1.1",
		"serve": "^14.2.1",
		"simplebar-react": "^3.0.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^6.0.0",
		"@graphql-codegen/client-preset": "^5.0.0",
		"@graphql-codegen/typescript": "^5.0.0",
		"@graphql-codegen/typescript-operations": "^5.0.0",
		"@graphql-codegen/typescript-react-query": "^6.1.1",
		"@types/node": "18.11.18",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"@types/react-lazy-load-image-component": "^1.5.2",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.0",
		"lint-staged": "^15.2.7",
		"prettier": "^2.8.3",
		"ts-node": "^10.9.1",
		"typescript": "4.9.4"
	},
	"engines": {
		"node": ">=18"
	}
}
